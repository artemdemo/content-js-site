{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["chrome >= 60"]
      }
    }]
  ],
  "plugins": [
    "transform-object-assign",
    "transform-class-properties",
    "syntax-dynamic-import",
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ]
}
